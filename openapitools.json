{
    "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 2,
    "generator-cli": {
        "version": "6.6.0",
        "generators": {
            "tour-operations": {
                "inputSpec": "https://tour-api-dev.tripota.com.vn/swagger/v1/swagger.json",
                "generatorName": "typescript-axios",
                "output": "sdk/tour-operations",
                "typeMappings": {
                    "DateTime": "Date"
                },
                "generateAliasAsModel": true,
                "apiPackage": "apis",
                "modelPackage": "models",
                "additionalProperties": {
                    "withSeparateModelsAndApi": true,
                    "stringEnums": true
                },
                "skipValidateSpec": true
            },
            "identity-next": {
                "inputSpec": "https://identity-next.tripota.com.vn/swagger/v1/swagger.json",
                "generatorName": "typescript-axios",
                "output": "sdk/identity-next",
                "typeMappings": {
                    "DateTime": "Date"
                },
                "generateAliasAsModel": true,
                "apiPackage": "apis",
                "modelPackage": "models",
                "additionalProperties": {
                    "withSeparateModelsAndApi": true,
                    "stringEnums": true
                },
                "skipValidateSpec": true
            }
        }
    }
}
